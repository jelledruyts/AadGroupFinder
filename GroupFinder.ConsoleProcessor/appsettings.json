{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "App": {
    "Processor": {
      "GroupSyncWaitTime": "01:00:00"
    },
    "AzureAD": {
      "Tenant": "[tenant.onmicrosoft.com]",
      "ClientId": "[client id]",
      "RedirectUri": "[redirect uri]",
      "TokenCacheFileName": "WebJobTokenCache.bin"
    },
    "AzureSearch": {
      "Service": "[search service name]",
      "Index": "[index name]",
      "AdminKey": "[admin key]"
    },
    "AzureStorage": {
      "Account": "[storage account name]",
      "Container": "[storage container name]",
      "AdminKey": "[admin key]"
    }
  }
}