{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": false,
    "target": "es5",
    "outFile": "../wwwroot/site.js"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}